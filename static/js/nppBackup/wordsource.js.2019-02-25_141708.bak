var app = function() {
	self.vue = {};
	
  Vue.config.silent = false; // show all warnings
	
	self.vue = new Vue({
		delimiters: ['${', '}'],
		unsafeDelimiters: ['!{', '}'],
		el: "#available_wordsources",
		data: {
			message: "Hello Vue!"
		}
	});
	return self;
}

var APP = null;

// This will make everything accessible from the js console;
// for instance, self.x above would be accessible as APP.x
jQuery(function(){APP = app();});